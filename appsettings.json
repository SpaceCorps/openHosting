{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "OpenHosting": "Debug"
    }
  },
  "Docker": {
    "DockerEndpoint": "npipe://./pipe/docker_engine",
    "DefaultPortRange": {
      "Start": 8000,
      "End": 9000
    }
  },
  "GitHub": {
    "WebhookSecret": "your-webhook-secret-here",
    "TempDirectory": "temp/repos"
  },
  "AllowedHosts": "*"
}
